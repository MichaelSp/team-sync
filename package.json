{
  "name": "team-sync",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub action to synchronize GitHub Teams with the contents of a teams document",
  "main": "dist/main.mjs",
  "types": "module",
  "scripts": {
    "build": "bun build --entrypoint src/main.ts --target node --outdir dist --sourcemap=external && mv dist/main.js dist/main.mjs && mv dist/main.js.map dist/main.mjs.map",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "bun test",
    "all": "bun run format && bun run lint && bun test && bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmacklin/team-sync.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "github",
    "teams"
  ],
  "author": "Richard Macklin",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^6.0.0",
    "@sindresorhus/slugify": "^2.2.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.0.4",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}